package controllers

import play.api._
import play.api.mvc.{Controller, Action}

object Application extends Controller {
  
  def index = Action {
    Ok("hello, world")
  }
  
  def tasks = Action { Ok("tasks")}
  
  def newTask = Action { Ok("newTask")}
  
  def deleteTask(id: Long) = Action {Ok("delete")}
}